{
  "name": "ascii-3d-animator",
  "version": "1.0.0",
  "description": "Transform 3D animated models into live ASCII art rendering at 60 FPS",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint js/ tests/ e2e/",
    "lint:fix": "eslint js/ tests/ e2e/ --fix",
    "format": "prettier --write \"**/*.{js,json,css,html,md}\"",
    "format:check": "prettier --check \"**/*.{js,json,css,html,md}\"",
    "docs": "jsdoc -c jsdoc.json"
  },
  "keywords": [
    "ascii",
    "3d",
    "threejs",
    "animation",
    "webgl"
  ],
  "author": "Zachary Sluss",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zacsluss/ascii_3d_animator.git"
  },
  "homepage": "https://zacsluss.github.io/ascii_3d_animator/",
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@playwright/test": "^1.40.0",
    "@vitest/coverage-v8": "^2.0.0",
    "@vitest/ui": "^2.0.0",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "jsdoc": "^4.0.0",
    "prettier": "^3.1.1",
    "vitest": "^2.0.0"
  }
}
